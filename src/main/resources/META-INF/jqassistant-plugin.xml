<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v1.8"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://schema.jqassistant.org/plugin/v1.8 https://schema.jqassistant.org/plugin/jqassistant-plugin-v1.8.xsd"
                    name="C#">
    <description>Provides a scanner for C# source code.</description>
    <model>
        <class>org.jqassistant.contrib.plugin.csharp.model.CSharpDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.CSharpClassesDirectoryDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.CSharpFileDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.CSharpClassDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.NamespaceDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.MethodDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.ConstructorDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.InvokesDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.EnumTypeDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.EnumDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.EnumValueDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.InterfaceTypeDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.ParameterDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.FieldDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.PrimitiveValueDescriptor</class>
        <class>org.jqassistant.contrib.plugin.csharp.model.UsesNamespaceDescriptor</class>
    </model>
    <scanner>
        <class>org.jqassistant.contrib.plugin.csharp.scanner.CSharpDirectoryScannerPlugin</class>
    </scanner>
</jqassistant-plugin>